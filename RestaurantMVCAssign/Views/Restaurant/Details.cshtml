@model RestaurantMVCAssign.Models.Restaurant
@{
    ViewBag.Title = "Details";
}

<h2>Restaurant Details</h2>

<div class="container">
    <div>
        <h3>Restaurant Id: @Model.RestaurantId</h3>
    </div>
    <div>
        <h3>Restaurant Name: @Model.RestaurantName</h3>
    </div>
    <div>
        <h3>Cusine: @Model.Cusine</h3>
    </div>

    <div>
        <a class="btn btn-default" href="/restaurant/index">Back</a>
    </div>
</div>